pipeline {
  agent any
 
  stages {
    stage('Checkout') {
      steps {
        git branch: 'main', url: 'https://github.com/chowdireddy/Jenkins-Zero-To-Hero.git'
      }
    }
    stage('Build and Test') {
      steps {
      sh 'cd java-maven-sonar-argocd-helm-k8s/spring-boot-app && mvn clean package'
      }
    }
   }
}
