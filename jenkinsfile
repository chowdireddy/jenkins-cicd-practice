pipeline {
  agent any
 tools { 
      maven 'MAVEN_HOME' 
      jdk 'JAVA_HOME'
  stages {
    stage('Checkout') {
      steps {
        git branch: 'master', url: 'https://github.com/chowdireddy/jenkins-cicd-practice.git'
      }
    }
    stage('Build and Test') {
      steps {
      sh 'mvn clean package'
      }
    }
   }
}
